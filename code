<script>
    let i,j; 
      for(i=0;i<9;i++)
      {
      for(j=0;j<=i;j++)
      {
          
          let num = (i+1)*(j+1);
          document.write((i+1)+"*"+(j+1)+"="+num)
          if(num>=10)
          {document.write("&nbsp&nbsp");}
          else
          {document.write("&nbsp&nbsp&nbsp&nbsp");}
      }
      document.write('<br/>');
      }
    
</script>
